{
  "name": "trpc-log",
  "version": "0.1.0",
  "description": "Lightweight request logging middleware for tRPC v11",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run"
  },
  "keywords": [
    "trpc",
    "logging",
    "middleware",
    "trpc-middleware",
    "request-logging"
  ],
  "author": "mgrom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mgrom/trpc-log"
  },
  "peerDependencies": {
    "@trpc/server": ">=11.0.0"
  },
  "devDependencies": {
    "@trpc/server": "^11.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  }
}
